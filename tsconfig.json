{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["ESNext", "DOM", "WebWorker"],
    "paths": {
      "@components/*": ["src/components/*"],
      "@data/*": ["src/_data/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
